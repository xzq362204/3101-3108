{
  "plugins": {
    "time_sanitizer": {
      "args": {
        "base_time_files": ["/usr/local/etc/lsb-factory",
                            "/etc/lsb-release"],
        "sync_period_secs": 5
      }
    },
    "disk_monitor": {
      "args": {
        "period_secs": 120,
        "stateful_usage_threshold": 90
      }
    },
    "battery_monitor": {
      "args": {
        "period_secs": 120,
        "critical_low_battery_pct": 5,
        "warning_low_battery_pct": 10
      }
    },
    "charge_manager": {
      "args": {
        "period_secs": 120,
        "min_charge_pct": 78,
        "max_charge_pct": 79
      }
    },
    "cpu_usage_monitor": {
      "args": {
        "period_secs": 120
      }
    },
    "cpu_freq_manager": {},
    "connection_manager": {
      "args": {
        "wlans": [
          {
            "ssid": "crosfactory20",
            "security": "psk",
            "passphrase": "crosfactory"
          },
          {
            "ssid": "crosfactory21",
            "security": "psk",
            "passphrase": "crosfactory"
          }
        ],
        "shuffle_wlans": true,
        "scan_wifi_periods_secs": 10
      }
    },
    "thermal_monitor": {
      "args": {
        "period_secs": 120,
        "delta_threshold": 1.1
      }
    },
    "core_dump_manager": {
      "args": {
        "period_secs": 120
      }
    },
    "instalog": {
      "args": {
        "uplink_hostname": "localhost",
        "uplink_port": 8086,
        "uplink_use_factory_server": true
      }
    },
    "device_manager": {},
    "status_monitor.status_monitor": {},
    "terminal.terminal": {},
    "log_rotation_checker": {
      "args": {
        "disable_rotation": true
      }
    }
  }
}
